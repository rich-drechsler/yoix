	if (data != NULL) {
	    switch (sortby ? sortby : GetDefaultSortby(selector)) {
		case SORTBY_INPUT_ORDER:
		    SortByInputOrder(selector, data);
		    break;

		case SORTBY_NAME:
		    SortByName(selector, data);
		    break;

		case SORTBY_ROWS:
		    SortByRows(selector, data);
		    break;

		case SORTBY_GROUP:
		    SortByGroup(selector, data);
		    break;
	    }
	}
	return(data);
    }

